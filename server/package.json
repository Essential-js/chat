{
	"scope": "essential-js",
	"name": "server",
	"version": "1.0.0",
	"title": "Chat server",
	"description": "Express Basic project",
	"license": "ISC",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"params": {
		"port": 5000,
		"client": "http://localhost:3001"
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "node",
				"environment": "development",
				"platform": "node",
				"ports": {
					"bundles": 6571
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.8",
		"@beyond-js/local": "~0.1.3",
		"socket.io-client": "^4.5.4",
		"express": "^4.17.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^16.11.7"
	}
}
